
<h1> Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
        <tr>
            <td><%= song.title%></td>
            <td><%= song.description%></td>
            <td>placeholder</td>
        </tr>
        <% end %>
    </tbody>
</table>