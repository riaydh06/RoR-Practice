
<h1> Songs</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
        <tr>
            <td><%= song.title%></td>
            <td><%= song.description%></td>
            <td><%= link_to "Show", song_path(song) %></td>
            <td><%= link_to "Edit", song_path(song) %></td>
            <td><%= link_to "Delete", song_path(song), method: :delete %></td>
        </tr>
        <% end %>
    </tbody>
</table>